@section Scripts {
    <script>
        $(document).ready(function () {

            // Set the toggleAll checkbox to checked on page load
            $('#toggleAll').prop('checked', false);
            jQuery('.rounded-circle').hide();


            // Call toggleAll to set the initial state for other checkboxes and textboxes
            toggleAll();

            // toggleAll click event
            $('#toggleAll').on('click', function () {
                jQuery('.rounded-circle').show();
                toggleAll();

            });

            function toggleAll() {
                const isChecked = $('#toggleAll').prop('checked');
                const isDisabled = isChecked;

                $('.toggle-checkbox').each(function () {
                    const targetSelector = $(this).data('target');

                    // Set the state of the checkbox itself
                    $(this).prop('checked', isChecked);

                    // Set the state of the target element
                    $(targetSelector).prop('disabled', isDisabled);
                });
            }

            function toggleField(checkbox) {
                const targetSelector = $(checkbox).data('target');
                $(targetSelector).prop('disabled', $(checkbox).is(':checked'));
            }

            $('.toggle-checkbox').on('click', function () {
                toggleField(this);
            });

            $('#GenerateJson').click(function () {
                function getFieldValue(id) {
                    if ($(`#${id}`).prop('disabled') === false) {
                        return $(`#${id}`).val() || undefined;
                    }
                }

                const payload = {
                    "allOrNone": true,
                    "compositeRequest": [
                        {
                            "method": "GET",
                            "referenceId": "thisCredentialingProfile",
                            "url": "/services/data/v52.0/sobjects/Credentialing_Profile__c/" + getFieldValue('credentialProfileId') + ""
                        },
                        {
                            "method": "PATCH",
                            "url": "/services/data/v59.0/sobjects/Credentialing_Profile__c/@@{thisCredentialingProfile.Id}",
                            "referenceId": "updateCredentialingProfile",
                            "body": {
                                "Accreditation_End__c": getFieldValue('Accreditation_End__c'),
                                "Accreditation_Start__c": getFieldValue('Accreditation_Start__c'),
                                "Accrediting_Body__c": getFieldValue('Accrediting_Body__c'),
                                "Proof_of_Accreditation_Status__c": getFieldValue('Proof_of_Accreditation_Status__c'),
                                "When_was_the_last_accreditation_survey__c": getFieldValue('When_was_the_last_accreditation_survey__c'),
                                "Date_of_Birth__c": getFieldValue('Date_of_Birth__c'),
                                "Tax_ID__c": getFieldValue('Tax_ID__c'),
                                "Medicare_Provider__c": getFieldValue('Medicare_Provider__c'),
                                "Medicare_Number__c": getFieldValue('Medicare_Number__c'),
                                "Practitioner_NPI__c": getFieldValue('Practitioner_NPI__c'),
                                "Office_Address_Street_1__c": getFieldValue('Office_Address_Street_1__c'),
                                "Office_Address_City__c": getFieldValue('Office_Address_City__c'),
                                "Office_Address_Zipcode__c": getFieldValue('Office_Address_Zipcode__c'),
