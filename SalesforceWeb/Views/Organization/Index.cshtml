                    <button id="addRowBtn" class="btn btn-primary mt-3">Add</button>
                    <button id="removeRowBtn" class="btn btn-danger mt-3">Remove</button>

                </div>
            </div>
        </div>
    </div>

    <div class="row pb-4">
        <div class="col-md-12 stretch-card">
            <div class="card">
                <div class="card-body">
                    <h6 class="card-title">JSON Result Editor</h6>
                    <form asp-controller="Organization" asp-action="PostCompositeOrganization" method="post" id="postOrganizationForm">
                        <div class="row">
                            <div class="col-sm-12">
                                <div class="mb-3">
                                    <textarea id="jsonEditor" name="jsonBody" class="form-control" rows="10"></textarea>
                                </div>
                            </div><!-- Col -->
                        </div><!-- Row -->

                        <div class="text-center pt-4">
                            <button type="submit" class="btn btn-primary">Post Json Result</button>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>

}


@section Scripts {
    <script>
        $(document).ready(function () {
            $('#GenerateJson').click(function () {
                $.ajax({
                    url: '@Url.Action("GetJsonData", "Organization")',
                    type: 'GET',
                    dataType: 'json',
                    success: function (response) {
                        if (response.success) {
                            $('#jsonEditor').val(response.data);
                        } else {
                            alert('Failed to fetch JSON data: ' + response.message);
                        }
                    },
                    error: function () {
                        alert('Error fetching JSON data.');
                    }
                });
            });
        });
    </script>
}

